{
  "name": "@sanalabs/y-json",
  "version": "0.1.7",
  "description": "Transform Yjs Shared Types according to a JSON object",
  "license": "ISC",
  "main": "dist/cjs/y-json/src/index.js",
  "module": "dist/esm/y-json/src/index.js",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build:cjs": "rm -rf dict/cjs && tsc --outDir dist/cjs --module commonjs",
    "build:esm": "rm -rf dist/esm && tsc --outDir dist/esm --module esnext",
    "build": "npm run build:cjs && npm run build:esm",
    "test": "jest"
  },
  "devDependencies": {
    "fast-check": "^2.19.0",
    "yjs": "^13.5.16"
  },
  "dependencies": {
    "@types/lodash": "^4.14.175",
    "jsondiffpatch": "^0.4.1",
    "lodash": "^4.17.21",
    "fast-json-patch": "^3.1.0"
  },
  "peerDependencies": {
    "yjs": "^13"
  }
}
